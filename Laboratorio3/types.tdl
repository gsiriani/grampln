sign := *top* & 
[ HEAD pos,
  SPR *list*,
  COMPS *list* ].

pos := *top*.
noun := pos.
verb := pos.
det := pos.
prep := pos.
adv := pos.
adj := pos.

phrase := sign &
[ ARGS *list* ].

word := sign &
[ ORTH string,
  MOD *list* ].

;;;;;;;;;;;;;;;;;;;;;;;

string := *top*.

*list* := *top*.

*ne-list* := *list* &
 [ FIRST *top*,
   REST *list* ].

*null* := *list*.


